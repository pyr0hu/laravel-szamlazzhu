{
  "name": "pyr0hu/laravel-szamlazzhu",
  "license": "MIT",
  "description": "Szamlazz.hu integration for Laravel",
  "keywords": [
    "laravel",
    "invoice",
    "receipt",
    "szamlazz.hu",
    "szamlazzhu",
    "currency"
  ],
  "authors": [
    {
      "name": "Forg√°cs Bence",
      "email": "sykorax93@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.0",
    "orchestra/testbench": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "mockery/mockery": "1.1.*"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "SzuniSoft\\SzamlazzHu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SzuniSoft\\SzamlazzHu\\Tests\\": "test/"
    }
  },
  "suggest": {
    "illuminate/validation": "^7.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    },
    "laravel": {
      "aliases": {
      },
      "providers": [
        "SzuniSoft\\SzamlazzHu\\Providers\\SzamlazzHuServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
